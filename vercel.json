{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/api/nytimes/:path*",
        "destination": "https://www.nytimes.com/svc/wordle/v2/:path*"
      },
      {
        "source": "/api/dictionary/:path*",
        "destination": "https://api.dictionaryapi.dev/api/v2/entries/en/:path*"
      }
    ]
  }
  